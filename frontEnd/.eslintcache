ute path = \"/main/manageUsers\" component = {ManageUsers}/>\n                <Route path = \"/main/movies\" component = {Movies}/>\n                <Route path = \"/main/subscriptions\" component = {Subscriptions}/>\n            </UserContext.Provider>\n         </div>\n        \n    ) \n}\n\nexport default Main\n","/Users/carmelklein/GitProjects/FullStackProject/frontEnd/src/Components/Subscriptions.js",["82"],"import {UserContext} from './Contexts'\nimport {useContext, useEffect} from 'react'\n\nconst Subscriptions  = () =>\n{\n    const context = useContext(UserContext)\n    \n    useEffect(() => {\n        console.log(\"Subscriptions: \"+context)\n\n      },[]);\n\n    return(\n       <div>\n       <h1>Subscriptions</h1>\n       </div>\n    ) \n}\n\nexport default Subscriptions\n","/Users/carmelklein/GitProjects/FullStackProject/frontEnd/src/Components/Movies.js",["83"],"import {UserContext} from './Contexts'\nimport {useContext, useEffect} from 'react'\n\nconst Movies  = () =>\n{\n\n    const context = useContext(UserContext)\n    useEffect(() => {\n        console.log(\"Movies: \"+context)\n\n      },[]);\n\n    return(\n       <div>\n       <h1>Movies</h1>\n       </div>\n    ) \n}\n\nexport default Movies\n","/Users/carmelklein/GitProjects/FullStackProject/frontEnd/src/Components/ManageUsers.js",["84","85"],"import {BrowserRouter as Router, Link , Switch, Route} from 'react-router-dom'\nimport Users from './Users'\nimport AddUser from './AddUser'\nimport EditUser from './EditUser'\nimport {useContext, useEffect} from 'react'\nimport {UserContext} from './Contexts'\n// import '../App.css'\nimport '../w3.css'\n\nconst ManageUsers  = () =>\n{\n    const context = useContext(UserContext)\n\n    useEffect(() => {\n        console.log(\"manager: \"+context)\n\n      },[]);\n    \n    return(\n        <div>\n        <ul className=\"w3-bar w3-black\">\n            <li className=\"w3-bar-item w3-button tablink\"><Link to = \"/main/manageUsers/users\">Users</Link></l