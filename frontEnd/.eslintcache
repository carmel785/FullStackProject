ptions watched\n            <ul>\n              <li></li>\n              <li></li>\n            </ul>\n          </div>\n          <br/><br/>\n          <input type =\"button\" value = \"Edit\" />\n          <input type =\"button\" value = \"Delete\"/>\n\n        </div> \n      })\n\n      \n    return(\n       <div >\n       Find Movie: <input type = \"text\" name = \"find\" />\n       <input type =\"button\" value = \"Find\"/> <br/><br/>\n       {items}\n       </div>\n    ) \n}\n\nexport default AllMovies\n","/Users/carmelklein/GitProjects/FullStackProject/frontEnd/src/Components/AddMovie.js",["124"],"import {UserContext} from './Contexts'\nimport {useContext, useEffect} from 'react'\n\nconst AddMovie  = () =>\n{\n\n    const context = useContext(UserContext)\n    useEffect(() => {\n        console.log(\"Add Movie: \"+context)\n\n      },[]);\n\n    return(\n       <div>\n       <h1>Add Movies</h1>\n       </div>\n    ) \n}\n\nexport default AddMovie\n","/Users/carmelklein/GitProjects/FullStackProject/frontEnd/src/Components/AllMembers.js",["125","126","127","128","129","130","131"],"/Users/carmelklein/GitProjects/FullStackProject/frontEnd/src/Components/AddMember.js",["132"],"import {UserContext} from './Contexts'\nimport {useContext, useEffect} from 'react'\n\nconst AddMember  = () =>\n{\n\n    const context = useContext(UserContext)\n    useEffect(() => {\n        console.log(\"Add Member: \"+context)\n        \n      },[]);\n\n    return(\n       <div>\n       <h1>Add Member</h1>\n       </div>\n    ) \n}\n\nexport default AddMember\n","/Users/carmelklein/GitProjects/FullStackProject/frontEnd/src/Components/SubscribeMovie.js",["133","134"],{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","severity":1,"message":"140","line":7,"column":26,"nodeType":"141","messageId":"142","endLine":7,"endColumn":32},{"ruleId":"139","severity":1,"message":"143","line":7,"column":33,"nodeType":"141","messageId":"142","endLine":7,"endColumn"